{% trans_default_domain global_theme %}

<!-- Navigation -->
<div class="list-group" id="sidebar">
    <div class="wow fadeInUp" data-wow-delay="0.2s" data-wow-offset="400">
    <a class="list-group-item page-scroll list-group-item-succes" href="#page-top">{{ config.settings.title }}</a>
    {% for active_section in config.settings.active_sections %}
        <a href="#{{ active_section }}" class="list-group-item">{{ active_section }}</a>
    {% endfor %}
    </div>
</div>